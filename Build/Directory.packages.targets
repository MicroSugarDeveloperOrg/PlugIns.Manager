<Project>
	<PropertyGroup>
		<!--Package-->
		<AntDesignVersion>11.0.0</AntDesignVersion>
		<AntDesignPackageOutputPath>$(RepositoryDirectory)nuget\</AntDesignPackageOutputPath>
		<VersionPrefix>avalonia11.0.0</VersionPrefix>
		<Authors>MicroSugarDeveloper</Authors>
		<Company>MicroSugarDeveloperOrg</Company>
		<Description>This repository is a set of styles that will help you customize your application in an appropriate ant-design.</Description>
		<PackageIcon>antdesign@avalonia-logo.png</PackageIcon>
		<PackageTags>avalonia xaml ant design theme colour color ui ux ant-design alibaba-ant-design</PackageTags>
		<RepositoryUrl>https://github.com/AvaloniaDeveloperOrg/AntDesign.Avalonia</RepositoryUrl>
		<PackageProjectUrl>https://github.com/AvaloniaDeveloperOrg/AntDesign.Avalonia</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Version>$(AntDesignVersion)</Version>
		<PackageOutputPath>$(AntDesignPackageOutputPath)</PackageOutputPath>
	</PropertyGroup>
</Project>